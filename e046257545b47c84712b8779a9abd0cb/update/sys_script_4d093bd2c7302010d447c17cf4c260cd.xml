        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var environments = [
		current.dev_environment.toString(), 
		current.test_environment.toString(), 
		current.prod_environment.toString(),
	];

    // filter out environments that are not set
	environments = environments.filter(function(val){return val !== '';});
	
	var arrayUtil = new global.ArrayUtil();
	
	if (environments.length !== arrayUtil.unique(environments).length){
		gs.addErrorMessage(gs.getMessage('Environments must be unique'));
		current.setAbortAction(true);
	}

})(current, previous);]]></script>
